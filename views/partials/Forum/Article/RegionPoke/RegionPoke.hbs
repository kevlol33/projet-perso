{{!-- Page Forum ou son regrouper les sujet --}}
<div class="container">
  <div class="row">
    <div class="col-md-12">
      <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/Home">Home</a></li>
          <li class="breadcrumb-item"><a href="/Forum">Forum</a></li>
          <li class="breadcrumb-item active" aria-current="page">Région Pokémon</li>
        </ol>
      </nav>
    </div>
  </div>
</div>

<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h1>Sujet sur les Région Pokémon</h1>
    </div>
  </div>
</div>
<div class="container">
  <div class="row">
    <div class="col-md-12">
      <table class="table table-hover table-dark">
        <thead>
          <tr>
            <th scope="col">Titre</th>
            <th scope="col">Description</th>
            <th scope="col">Bouton</th>
          </tr>
        </thead>
        {{#each dbType1 }}
        <tr>
          <td>{{ title }}</td>
          <td>{{ description }}</td>
          <td> <a href="/Sujet/{{_id}}" class="btn btn btn-outline-success"><i class="fas fa-eye"></i></a></td>
        </tr>
        {{/each }}
      </table>
    </div>
  </div>
</div>