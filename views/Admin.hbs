<h1 class="text-center">
                <i class="fas fa-user-secret"></i>
                Administration</h1>
            <div id="accordion">
                <div class="card">
                    <div class="card-header" id="Article">
                        <h5 class="mb-0">
                            <button class="btn btn-link" data-toggle="collapse" data-target="#collapseArticle"
                                aria-expanded="false" aria-controls="collapseArticle">
                                <i class="fas fa-cogs"></i>
                                Gestion des Sujet
                            </button>
                        </h5>
                    </div>

                    <div id="collapseArticle" class="collapse show" aria-labelledby="Article" data-parent="#accordion">
                        <div class="card-body">
                            {{> Admin/Btn}}
                            {{> Admin/AdminSujet}}
                        </div>
                    </div>
                </div>
                <div class="card">
                    <div class="card-header" id="Sujet">
                        <h5 class="mb-0">
                            <button class="btn btn-link collapsed" data-toggle="collapse" data-target="#collapseSujet"
                                aria-expanded="false" aria-controls="collapseSujet">
                                <i class="fas fa-user-cog"></i>
                                Gestoin des Utilisateur
                            </button>
                        </h5>
                    </div>
                    <div id="collapseSujet" class="collapse" aria-labelledby="Sujet" data-parent="#accordion">
                        <div class="card-body">
                            {{> Admin/AdminUser}}
                        </div>
                    </div>
                </div>
            </div>
            

